<h1> Add a New Shelter Listing </h1>
<div class='center-form'>
<h2> Enter the following information: </h2>
<%= form_tag '/shelters' do %>
  <%= label_tag 'Name: '%>
  <%= text_field_tag :name %><br/>
  <%= label_tag 'Address :' %>
  <%= text_field_tag :address %><br/>
  <%= label_tag 'City: '%>
  <%= text_field_tag :city %><br/>
  <%= label_tag 'State: ' %>
  <%= text_field_tag :state %><br/>
  <%= label_tag 'Zip Code: '%>
  <%= text_field_tag :zip_code %><br/>
<%= submit_tag 'Submit' %>
<% end %>
</div>
